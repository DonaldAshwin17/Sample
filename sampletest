// dialog.index.test.js
import * as DialogActions from './dialog.actions';
import reducer from './dialog.reducer';
import * as DialogModule from './dialog.index'; // Adjust the path as per your file structure

describe('dialog.index.js', () => {
  it('should export openDialog and closeDialog from dialog.actions', () => {
    expect(DialogModule.openDialog).toBe(DialogActions.openDialog);
    expect(DialogModule.closeDialog).toBe(DialogActions.closeDialog);
  });

  it('should export reducer as default', () => {
    expect(DialogModule.default).toBe(reducer);
  });
});
